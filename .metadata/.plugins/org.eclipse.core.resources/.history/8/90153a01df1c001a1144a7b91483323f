package data;

import org.newdawn.slick.opengl.Texture;
import static helpers.Grafika.*;

public enum TipProjektila {

	metakVatraM(QuickLoad("metakVatraMali"), 10, 500),
	MetakVatraV(QuickLoad("metakVatraVeliki"), 30, 500),
	MetakVatraCrveni(QuickLoad("metakLed"), 5, 400),
	BulletRed(QuickLoad("metakCrveni"), 50, 500);
	
	
	Texture tekstura;
	int snagaProjektila;
	float brzinaProjektila;
	
	TipProjektila(Texture tekstura, int snaga, float brzina) {
		this.tekstura = tekstura;
		this.snagaProjektila = snaga;
		this.brzinaProjektila = brzina;
	}
	
}
