package data;

public enum KonstantaPlocica {
	
	Grass("grass64", true),
	Dirt("dirt64", false),
	Water("water64", false),
	NULL("water64", false);
	
	String textureName;
	boolean buildable;
	
	KonstantaPlocica(String textureName, boolean buildable) {
		this.textureName = textureName;
		this.buildable = buildable;
	}

}
