package data;

public class TockaProvjerePutanje {
	
	private Plocica plocica;
	private int xSmjer, ySmjer;
	
	public TockaProvjerePutanje(Plocica tile, int xSmjer, int ySmjer) {
		this.plocica = tile;
		this.xSmjer = xSmjer;
		this.ySmjer = ySmjer;
	}

	public Plocica dohvatiPlocicu() {
		return this.plocica;
	}

	public int dohvatiXSmjer() {
		return this.xSmjer;
	}

	public int dohvatiYSmjer() {
		return this.ySmjer;
	}
	
}
