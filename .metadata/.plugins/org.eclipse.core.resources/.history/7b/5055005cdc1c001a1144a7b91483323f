package data;

public class TockaProvjerePutanje {
	
	private Plocica tile;
	private int xSmjer, yDirection;
	
	public TockaProvjerePutanje(Plocica tile, int xDirection, int yDirection) {
		this.tile = tile;
		this.xSmjer = xDirection;
		this.yDirection = yDirection;
	}

	public Plocica getTile() {
		return tile;
	}

	public int getxDirection() {
		return xSmjer;
	}

	public int getyDirection() {
		return yDirection;
	}
	
}
