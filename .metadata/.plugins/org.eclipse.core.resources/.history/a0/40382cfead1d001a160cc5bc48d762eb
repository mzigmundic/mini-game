package com.glavni;

public class ProjektilLed extends Projektil {

	public ProjektilLed(TipProjektila tip, Neprijatelj target, float x, float y, int width, int height) {
		super(tip, target, x, y, width, height);
	}
	
	@Override
	public void nanesiStetu() {
		super.getTarget().postaviBrzinu(30f);
		super.nanesiStetu();
	}

}
